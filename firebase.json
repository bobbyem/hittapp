{
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint"
    ],
    "source": "functions"
  },
  "hosting": {
    "redirects": [
      {
        "source": "/hittat",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/tappat",
        "destination": "/",
        "type": 301
      }
    ],
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
